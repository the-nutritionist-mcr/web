{
  "root": "apps/web-app",
  "sourceRoot": "apps/web-app",
  "projectType": "application",
  "targets": {
    "deploy:cypress": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cdk deploy tnm-web-cypress-stack --require-approval never --outputs-file app-config.json",
        "cwd": "apps/web-app"
      }
    },
    "destroy": {
      "executor": "@codebrew/nx-aws-cdk:destroy",
      "options": {}
    },
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": [
        "{options.outputPath}",
        "dist/apps/web-app/out_lambda"
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/web-app",
        "outputPath": "dist/apps/web-app"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "web-app:build",
        "dev": true
      },
      "configurations": {
        "production": {
          "buildTarget": "web-app:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "web-app:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/web-app"
      ],
      "options": {
        "jestConfig": "apps/web-app/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/web-app/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
